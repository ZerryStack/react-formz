    "zod": "^3.19.1"
  },
  "peerDependencies": {
    "@zerry/react-formz": ">=1.1.0",
    "@zerry/react-formz-native": ">=1.1.0",
    "zod": ">=3.0.0"
  },
  "peerDependenciesMeta": {
