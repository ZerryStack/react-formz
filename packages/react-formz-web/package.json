{
  "name": "@zerry/react-formz",
  "version": "1.2.0",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "types": "./dist/react-formz-web/src/index.d.ts",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "source": "./src/index.ts",
  "keywords": [
    "react",
    "forms",
    "javascript",
    "typescript",
    "form-builder"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit",
    "clean:dist": "rm -rf ./dist",
    "build": "yarn clean:dist && yarn typecheck && yarn build:src && yarn build:types",
    "build:src": "vite build",
    "build:types": "tsc -P tsconfig.build.json",
    "build:documentation": "yarn workspace @zerry/react-formz-documentation build",
    "prepublishOnly": "yarn test && yarn core:test && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:coverage": "jest --coverage=true",
    "core:test": "yarn workspace @zerry/react-formz-core test",
    "native:test": "yarn workspace @zerry/react-formz-native test"
  },
  "dependencies": {
    "@zerry/react-formz-core": "workspace:^",
    "react-uuid": "^1.0.3",
    "zustand": "^4.1.1"
  },
  "peerDependencies": {
    "immer": ">=7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.0",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "ramda": "^0.28.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
