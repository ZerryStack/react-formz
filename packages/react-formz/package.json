{
  "name": "@zerry/react-formz",
  "version": "0.2.1-beta",
  "packageManager": "yarn@3.2.3",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "source": "./src/index.ts",
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@mdx-js/react": "^1.6.22",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.0.0",
    "@types/ramda": "^0.28.15",
    "@types/react": "~18.0.0",
    "@types/react-dom": "~18.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "ansi-colors": "^4.1.3",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "date-fns": "^2.29.2",
    "debug": "^4.3.4",
    "immer": "^9.0.15",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "ramda": "^0.28.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "typecheck": "tsc --noEmit",
    "build": "rm -rf ./dist && yarn typecheck && vite build && yarn build:types && yarn build:documentation",
    "build:types": "tsc -P tsconfig.build.json",
    "build:documentation": "yarn workspace @zerry/react-formz-documentation build",
    "prepublishOnly": "yarn test && yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:coverage": "jest --coverage=true"
  },
  "dependencies": {
    "react-uuid": "^1.0.3",
    "zustand": "^4.1.1"
  },
  "peerDependencies": {
    "immer": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZerryStack/react-formz.git"
  }
}
